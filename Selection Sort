#include<stdio.h>
#include<stdlib.h>
void selectsort(int A[],int n)
{
    int i,j,k;
    for(int i=0;i<n-1;i++)
    {
        for(j=k=i;j<n;j++)
        {
            if(A[j]<A[k])
            {
                k=j;
            }
            int temp=A[i];
            A[i]=A[k];
            A[k]=temp;
        }
    
    }    
}    
int main()
{
    int n=7;
    int A[7]={5,6,4,3,7,12,90};
    for(int i=0;i<n;i++)
    {
        printf("%d ",A[i]);
    }
    printf("\n");
    selectsort(A,n);
    for(int i=0;i<n;i++)
    {
        printf("%d ",A[i]);
    }
    return 0;
}
