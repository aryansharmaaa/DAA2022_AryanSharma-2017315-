#include <stdio.h>
#include <stdlib.h>
void insertsort(int a[],int n)
{
    int t;
    for(int i=1;i<n;i++)
    {
        int j=i-1;
        t=a[i];
        while(a[j]>t && j>=0)
        {
            a[j+1]=a[j];
            j--;
        }
        a[j+1]=t;
    }
}
int main()
{
    int n=7,a[]={34,56,89,34,21,76,23};
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
    printf("\n");
    printf("after sorting: ");
    insertsort(a,n);
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
    return 0;
}
